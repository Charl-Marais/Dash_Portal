
<!-- include all the html tags before an including the wrapper div tag //-->
<% include partials/html_head %>


<% include partials/main_header %>


<% include partials/main_sidebar %>

<!-- Content Wrapper. Contains page content -->
  <div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
      <h1>
        User Management
      </h1>
      <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Level</a></li>
        <li class="active">Here</li>
      </ol>
    </section>

    <!-- Main content -->
    <section class="content container-fluid">
      <div class="box">
        <div class="box-header">
          <h3 class="box-title">List Of users</h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body no-padding">
          <table class="table table-striped">
            <tbody><tr>
              <th style="width: 10px">#</th>
              <th>Username</th>
              <th>First Name</th>
              <th>Last Name</th>
              <th>Title</th>
              <th>Email</th>
              <th>Mobile Number</th>
              <th>Office Number</th>
              <th>Presence</th>
              <th>Country</th>
              <th>City</th>
              <th>Campus</th>
              <th>Building</th>
              <th>Floor</th>
            </tr>
            <% users.forEach( (u) => { %>
              <tr>
                <td><%= u.id %>.</td>
                <td><%= u.username %></td>
                <td><%= u.firstName %></td>
                <td><%= u.lastName %></td>
                <td><%= u.title %></td>
                <td><%= u.email %></td>
                <td><%= u.mobileNumber %></td>
                <td><%= u.officeNumber %></td>
                <td><%= u.presence %></td>
                <td><%= u.country %></td>
                <td><%= u.city %></td>
                <td><%= u.campus %></td>
                <td><%= u.building %></td>
                <td><%= u.floor %></td>
              </tr>
            <% }) %>
            </tbody>
          </table>
        </div>
      <!-- /.box-body -->
      </div>
    </section>
    <!-- /.content -->
  </div>
  <!-- /.content-wrapper -->


<% include partials/main_footer %>


<% include partials/control_sidebar %>

